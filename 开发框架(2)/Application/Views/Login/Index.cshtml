@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta http-equiv="Expires" content="0" />
    <meta http-equiv="Cache-Control" content="no-cache" />
    <meta http-equiv="Pragma" content="no-cache" />
    <meta name="viewport" content="width=device-width" />
    <title>唐山市人才大数据-登录</title>
    <link href="~/css/bootstrap-3.3.7-dist/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/css/common.css" rel="stylesheet" />
    <script src="~/js/jquery-1.10.2.min.js"></script>
    <script src="~/js/json2.js"></script>
    <script src="~/css/bootstrap-3.3.7-dist/js/bootstrap.min.js"></script>
    <script src="~/js/vue.js"></script>
    <script src="~/js/cathyJs.js"></script>
    <link href="~/login/index.css" rel="stylesheet" />
    <link href="~/lib/element-ui/index.css" rel="stylesheet" />
    <script src="~/lib/element-ui/index.js"></script>
    <script src="~/js/axios.js"></script>
    <script src="~/js/config.js"></script>
    <script src="~/js/jsencrypt.min.js"></script>
</head>
<body>
    <div class="loginKuang">
        <div class="loginInfo">
            <table style="">
                <tr>
                    <td height="20px"></td>
                    <td style="color:red;" id="tdErrInfo"></td>
                </tr>
                <tr>
                    <td height="40px">用户名：&nbsp;&nbsp;&nbsp;&nbsp;</td>
                    <td>
                        <input type="text" class="form-control" id="inputUserName" placeholder="请输入用户名" style="width:250px;" />
                    </td>
                </tr>
                <tr>
                    <td height="40px">密&nbsp;&nbsp;&nbsp;&nbsp;码：&nbsp;&nbsp;&nbsp;&nbsp;</td>
                    <td>
                        <input type="password" class="form-control" id="inputPassWord" placeholder="请输入密码" style="width:250px;" />
                    </td>
                </tr>
                <tr>
                    <td height="40px">验证码：&nbsp;&nbsp;&nbsp;&nbsp;</td>
                    <td>
                        <input type="text" class="form-control" id="inputValidateCode" placeholder="请输入验证码" style="width:150px;display:inline-block;" />&nbsp;&nbsp;<input type="button" id="code" onclick="createCode()" />
                    </td>
                </tr>
                <tr>
                    <td height="50px"></td>
                    <td style="text-align:left">
                        <button type="button" class="btn btn-primary" style="width:100px;height:35px;" onclick="userLogin();">登录</button>&nbsp;&nbsp;
                        <button type="button" class="btn btn-primary" style="width:100px;height:35px;" onclick="userClear();">取消</button>
                    </td>
                </tr>
            </table>
        </div>
    </div>
    <script type="text/javascript">
        document.writeln("<script src='/login/index.js?rnd=" + Math.random() + "'></s" + "cript>");
    </script>
</body>
</html>