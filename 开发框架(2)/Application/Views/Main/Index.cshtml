@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta http-equiv="Expires" content="0" />
    <meta http-equiv="Cache-Control" content="no-cache" />
    <meta http-equiv="Pragma" content="no-cache" />
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>人才大数据-后台管理</title>
    <link href="~/css/bootstrap-3.3.7-dist/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/css/bootstrap-select/css/bootstrap-select.min.css" rel="stylesheet" />
    <link href="~/css/bootstrap-select/css/extBootstrapSelct.css" rel="stylesheet" />
    <link href="~/js/pagination_zh/lib/pagination.css" rel="stylesheet" />
    <link href="~/lib/element-ui/index.css" rel="stylesheet" />
    <link href="~/css/common.css" rel="stylesheet" />
    <link href="~/js/nprogress/nprogress.css" rel="stylesheet" />
    <script src="~/js/jquery-1.10.2.min.js"></script>
    <script src="~/js/My97DatePicker/WdatePicker.js"></script>
    <script src="~/js/pagination_zh/lib/jquery.pagination.js"></script>
    <script src="~/css/bootstrap-3.3.7-dist/js/bootstrap.min.js"></script>
    <script src="~/css/bootstrap-select/js/bootstrap-select.min.js"></script>
    <script src="~/css/bootstrap-select/js/SelectExtend.js"></script>
    <script src="~/js/json2.js"></script>
    <script src="~/js/vue.js"></script>
    <script src="~/js/cathyJs.js"></script>
    <script src="~/js/pageCommend.js"></script>
    <script src="~/js/jqueryExtend.js"></script>
    <script src="~/js/nprogress/nprogress.js"></script>
    <link href="~/css/jquery.mCustomScrollbar.css" rel="stylesheet" />
    <script src="~/lib/element-ui/index.js"></script>
    <script src="~/js/axios.js"></script>
    <link href="~/css/elInner.css" rel="stylesheet" />
    <link href="~/main/index.css" rel="stylesheet" />

</head>
<body>
    <div id="app">
        <div class="top">
            <div class="logo">
                <img src="/Images/logoRed.png" style="height: 55px;" />&nbsp;
                唐山人才大数据系统
            </div>
            <div class="topRight">
                <el-dropdown>
                    <span class="el-dropdown-link" style="color:#ffffff;">
                        欢迎您：<span>{{userName}}</span>
                    </span>
                    <template #dropdown>
                        <el-dropdown-menu>
                            <el-dropdown-item v-on:click.native="editPassword()">修改密码</el-dropdown-item>
                            <el-dropdown-item v-on:click.native="backSystem()">退出系统</el-dropdown-item>
                        </el-dropdown-menu>
                    </template>
                </el-dropdown>
            </div>
        </div>
        <div id="divLeft" class="left">
            @*菜单Begin*@
            <div class="leftMenu">
                
            </div>
            @*菜单End*@
        </div>
        <div id="divRight" class="right">
            <iframe src="/SystemManage/Manager" frameborder="0" scrolling="auto" id="ifrMain" name="ifrMain" width="100%"></iframe>
        </div>
        <!--修改密码Begin-->
        <el-dialog title="修改密码" v-model="dialogFormVisible" width="550px"  >
            <el-form :model="passInfo" :rules="rules" label-width="100px" ref="passInfo" size="small">
                <el-form-item label="原密码" prop="oldPass">
                    <el-input v-model="passInfo.oldPass" type="password" show-password="true"></el-input>
                </el-form-item>
                <el-form-item label="新密码" prop="newPass">
                    <el-input v-model="passInfo.newPass" type="password" show-password="true"></el-input>
                </el-form-item>
                <el-form-item label="新密码确认" prop="newConfirmPass">
                    <el-input v-model="passInfo.newConfirmPass" type="password" show-password="true"></el-input>
                </el-form-item>
            </el-form>
            <template #footer>
                <span class="dialog-footer">
                    <el-button v-on:click="dialogFormVisible = false" size="small">取 消</el-button>
                    <el-button type="primary" v-on:click="editPasswordEvent('passInfo')"  size="small">确 定</el-button>
                </span>
            </template>
        </el-dialog>
        <!--修改密码End-->
    </div>
    <script src="~/js/jquery-ui-1.10.4.min.js"></script>
    <script src="~/js/jquery.mousewheel.min.js"></script>
    <script src="~/js/jquery.mCustomScrollbar.js"></script>
    <script type="text/javascript">
        document.writeln("<script src='/main/index.js?rnd=" + Math.random() + "'></s" + "cript>");
    </script>
</body>
</html>
