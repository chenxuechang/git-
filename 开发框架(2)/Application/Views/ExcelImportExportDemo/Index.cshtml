@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta http-equiv="Expires" content="0" />
    <meta http-equiv="Cache-Control" content="no-cache" />
    <meta http-equiv="Pragma" content="no-cache" />
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>excel导入导出</title>
    <link href="~/lib/element-ui/index.css" rel="stylesheet" />
    <script src="~/js/jquery-1.10.2.min.js"></script>
    <script src="~/js/json2.js"></script>
    <script src="~/js/vue.js"></script>
    <script src="~/js/cathyJs.js"></script>
    <script src="~/js/jqueryExtend.js"></script>
    <script src="~/lib/element-ui/index.js"></script>
    <script src="~/js/axios.js"></script>
    <link href="~/css/elInner.css" rel="stylesheet" />
    <link href="~/main/index.css" rel="stylesheet" />
</head>
<body style="background:#ffffff;">
    <div id="app">
        <el-upload class="upload-demo"
                   ref="upload"
                   action="/api/ExcelImportExportDemoApi/ImportExcel"
                   accept=".xls,.xlsx"
                   :on-remove="handleRemove"
                   :on-success="fileUploadSuccess"
                   :on-error="fileUploadFail"
                   :on-change="fileChange"
                   :file-list="fileList"
                   :auto-upload="false">
            <template #trigger>
                <el-button size="small" type="primary">选取文件</el-button>
            </template>
            <el-button style="margin-left: 10px; display:none;" size="small" type="success" v-on:click="submitUpload">上传到服务器</el-button>
            <template #tip>
                <div class="el-upload__tip">
                    只能上传excel（.xls, .xlsx）文件
                </div>
            </template>
        </el-upload>

        <el-button style="margin-left: 10px;" size="small" type="success" v-on:click="submitUpload">上传到服务器</el-button>

        <el-button style="margin-left: 50px;" size="small" type="primary" v-on:click="exportExcel">导出EXCEL</el-button>

        <iframe id="ifrDownload" :src="excelDownPath" height="0" width="0" title="fileDown" placeholder="" frameborder="0"></iframe>
    </div>
    <script type="text/javascript">
        document.writeln("<script src='/excelImportExportDemo/index.js?rnd=" + Math.random() + "'></s" + "cript>");
    </script>
</body>
</html>
